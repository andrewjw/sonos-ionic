{
    "extends": ["tslint:latest", "tslint-config-prettier", "tslint-plugin-prettier"],
    "rules": {
        "prettier": true,
        "max-line-length": {
            "options": [120]
        },
        "no-console": {
            "severity": "warning",
            "options": [
                "debug",
                "info",
                "log",
                "time",
                "timeEnd",
                "trace"
            ]
        },
        "interface-name": [false],
        "no-implicit-dependencies": false
    }
}
